package Model;

public enum CardType {
	
	SAVING(1),
	LOAN(2),
	INVESTMENT(3);
	
	private final int value;
	
	CardType(int value){
		this.value = value;
	}
	
	public int getValue() {
		return this.value;
	}
	
	public static CardType fromValue(int value) {
		for(CardType type : CardType.values()) {
			if(type.getValue() == value) {
				return type;
			}
		}
		throw new IllegalArgumentException();                
	}
}
